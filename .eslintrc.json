{
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/prop-types": 0,
      "no-unused-vars": "warn",
      "no-console": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "react/react-in-jsx-scope": "off"
    }
  }